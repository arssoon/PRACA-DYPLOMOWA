<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="1000.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.VisualisationController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <Pane prefHeight="800.0" prefWidth="1000.0">
               <children>
                  <Button fx:id="startCaptureButton" layoutX="640.0" layoutY="34.0" mnemonicParsing="false" onAction="#StartCapturePacket" text="Start Capture" />
                  <Button fx:id="stopCaptureButton" layoutX="640.0" layoutY="77.0" mnemonicParsing="false" onAction="#StopCapturePacket" text="Stop Capture" />
                  <ChoiceBox fx:id="networkDevicesBox" layoutX="225.0" layoutY="34.0" prefHeight="31.0" prefWidth="402.0" />
                  <TextArea fx:id="textAreaOutput" layoutY="133.0" prefHeight="276.0" prefWidth="1000.0" />
                  <Button fx:id="listNetworkDevices" layoutX="225.0" layoutY="77.0" mnemonicParsing="false" onAction="#ListNetworkInterfaces" prefHeight="31.0" prefWidth="402.0" text="List Network Devices" />
                  <TextArea fx:id="textAreaShow" layoutX="-3.0" layoutY="426.0" prefHeight="362.0" prefWidth="1000.0" />
               </children>
            </Pane>
      </children>
    </AnchorPane>
  </children>
</VBox>
